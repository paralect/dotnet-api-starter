    // "ConnectionString": "Server=localhost;Port=5432;Database=api-development;User Id=postgres;Password=pwd;Pooling=true;" // for local instance
    "ConnectionString": "Host=postgres;Database=api-development;Username=postgres;Password=rootPassword" // for docker-compose
  },
  "App": {
    "WebUrl": "http://localhost:3002",
    "LandingUrl": "http://localhost:3000",
    "AuthorizationDatabase": 1 // 1 - Mongo, 2 - Postgres
  },
  "AllowedHosts": "*"
}