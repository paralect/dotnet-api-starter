{
  "Google": {
    "ClientId": "clientId",
    "ClientSecret": "clientSecret",
    "RedirectUrl": "redirectUrl"
  },
  "TokenExpiration": {
    "AccessTokenExpiresInHours": 1,
    "RefreshTokenExpiresInHours": 10
  },
  "MongoConnection": {
    // "ConnectionString": "mongodb://localhost:27017", // for local instance
    "ConnectionString": "mongodb://root:rootPassword@mongo:27017", // for docker-compose
    "Database": "api-development"
  },
